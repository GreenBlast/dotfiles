# Ignore everything
/*

# Ignore vim temp files
*~
*.swp
*.swo

# Except the gitignore
!/.gitignore

# Ignore specific paths in config dir
!.config
.config/*
!.config/i3
!.config/scripts
!.config/zsh
.config/zsh/local_aliases.zsh
.config/zsh/config.zsh
!.config/nvim
.config/nvim/*
!.config/nvim/colors
!.config/ranger
.config/ranger/bookmarks
.config/ranger/history
.config/ranger/tagged

# Exclude these directories
!.fzf
!.oh-my-zsh
!.scripts
!.spaceship-zsh-theme
!.tmux/plugins/tpm
!.yadm
!vimwiki
!zsh_custom/plugins/zsh-syntax-highlighting
!zsh_custom/plugins/zsh-autosuggestions

# And this files
!.gitmodules
!.config/nvim/init.vim
!.tmux.conf
!.warprc##*
!.Xdefaults
!.Xresources
!.zshrc
!LICENSE
!pylintrc
!README.md

# Do ignore this subdirectories
.yadm/repo.git
.scripts/work


# Do ignore these files
/vimwiki/Work notes.wiki
/vimwiki/Courses.wiki

